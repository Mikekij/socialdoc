<script>
  $(function () {
      $('#<%= target_div %>').highcharts({
          title: {
            text: null,
          },
          xAxis: {
              categories: <%= data.map {|x| x[0]} %>
          },
          yAxis: {
              min: 0,
              title: {
                  text: '<%= y_axis_text %>'
              },
              plotLines: [{
                  value: 0,
                  width: 1,
                  color: '#808080'
              }]
          },
          plotOptions: {
            series: {
                marker: {
                    enabled: false
                }
            }
          },
          legend: {
              enabled:false
          },
          series: [{
              name: '5:1 Ratio',
              data: <%= data.map {|x| x[1]} %>
          }]
      });
  });
</script>
